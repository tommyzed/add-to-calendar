{
    "name": "Screenshot to Calendar",
    "short_name": "CalSnap",
    "start_url": "/",
    "display": "standalone",
    "background_color": "#ffffff",
    "theme_color": "#ffffff",
    "icons": [
        {
            "src": "/vite.svg",
            "sizes": "192x192",
            "type": "image/svg+xml"
        }
    ],
    "share_target": {
        "action": "/share-target",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "title": "title",
            "text": "text",
            "url": "url",
            "files": [
                {
                    "name": "media",
                    "accept": [
                        "image/*",
                        ".png",
                        ".jpg",
                        ".jpeg",
                        ".webp"
                    ]
                }
            ]
        }
    }
}